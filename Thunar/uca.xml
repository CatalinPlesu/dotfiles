<?xml version="1.0" encoding="UTF-8"?>
<actions>
<action>
	<icon>rawtherapee</icon>
	<name>Custom folder icon</name>
	<unique-id>1629884744556410-2</unique-id>
	<command>sxiv -fqot ~/.local/share/icons/thunar_icons  | xargs -I{} cp {} %f/.folder.png</command>
	<description>opens sxiv to select with m a new folder icon</description>
	<patterns>*</patterns>
	<startup-notify/>
	<directories/>
</action>
<action>
	<icon>user-trash-full</icon>
	<name>default folder icon</name>
	<unique-id>1629884744556410-3</unique-id>
	<command>rm %f/.folder.png</command>
	<description>removes the .folder file </description>
	<patterns>*</patterns>
	<startup-notify/>
	<directories/>
</action>
</actions>
